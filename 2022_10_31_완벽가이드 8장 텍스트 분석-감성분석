      "cell_type": "code",
      "source": [
        "import nltk\n",
        "from nltk.corpus import sentiwordnet as swn\n",
        "\n",
        "father = swn.senti_synset('father.n.01')\n",
        "print('father 긍정감성 지수: ',father.pos_score())\n",
        "print('father 부정감성 지수: ',father.neg_score())\n",
        "print('father 객관성 지수: ',father.obj_score())\n",
