      "source": [
        "#### 63. Create an array class that has a name attribute (★★☆)"
      ],
      "id": "f4427f7f"
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "fe253212",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "834e331d-a173-449e-85d5-62335bf905f7"
      },
      "source": [
        "class NameArray(np.ndarray) : \n",
        "  def __new__(cls, array, name = \"no name\"):\n",
        "    obj = np.asarray(array).view(cls)\n",
        "    obj.name = name\n",
        "    return obj\n",
        "  def __array_finalize__(self, obj) :\n",
        "    if obj is None : return \n",
        "    self.info = getattr(obj, 'name', 'no name')\n",
        "z = NameArray(np.arange(10), 'range_10')\n",
        "print(z.name)"
      ],
      "id": "fe253212",
      "execution_count": 68,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "range_10\n"
          ]
        }
      ]
    },
